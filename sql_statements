CREATE DATABASE visual_codus;
\c visual_codus;
CREATE TABLE students (student_id SERIAL PRIMARY KEY, course_id int, age int, gender varchar, origin varchar, distance_traveled varchar, salary_before int);
CREATE TABLE courses (course_id SERIAL PRIMARY KEY, course_name varchar);
CREATE DATABASE visual_codus_test WITH TEMPLATE visual_codus;
